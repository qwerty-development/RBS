{
  "cli": {
    "version": ">= 12.1.0",
    "appVersionSource": "remote"
  },

  "build": {
    "dev": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "dev"
    },


    "production": {
      "developmentClient": false,      
      "distribution": "internal",
      "channel": "production"
    },


    "store": {
      "autoIncrement": true,
      "distribution": "store",
      "channel": "production"
    }
  },

  "submit": {
    "store": {}
  }
}
